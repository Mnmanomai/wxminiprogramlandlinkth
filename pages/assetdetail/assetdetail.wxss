/* pages/assetdetail/assetdetail.wxss */
.HeaderContent {
  padding-inline: 20rpx;
  padding-block: 20rpx;

}

.contentDetail {
  padding-left: 20rpx;
  padding-right: 20rpx;
}

.text-muted {
  color: rgb(122, 122, 122);
}

.section {
  margin: 10px 0px;
}

.Header {
  font-size: 20px;
  font-weight: 700;
  color: rgb(2, 2, 73);
}

.SubHeader {
  font-size: 18px;
  font-weight: 500;
  color: rgb(35, 35, 65);
}

.Contactme {
  position: fixed;
  bottom: 0px;
  height: 60px;
  width: 100%;
  background-color: rgb(26, 25, 94);
  z-index: 10;
  align-items: center;
  justify-items: center;
}

.iconcontact {
  width: 60rpx;
  height: 60rpx;
  padding: 6px;
  border-radius: 12px;
  background-color: white;
}

.carousel-container {
  width: 100%;
  height: 300px;
  position: relative;
  overflow: hidden;
}

.carousel {
  width: 100%;
  height: 100%;
}

.carousel-image {
  width: 100%;
  height: 100%;
  display: block;
}

.tagBath {
  background-color: rgba(169, 169, 169, 0.2);
  color: rgb(169, 169, 169);
  font-size: 10px;
  padding-left: 2px;
  padding-right: 2px;
  border-radius: 2px;
}

.tagHeading {
  background-color: rgba(169, 169, 169, 0.2);
  color: rgb(169, 169, 169);
  font-size: 11px;
  padding-left: 4px;
  padding-right: 4px;
  padding-top: 2px;
  padding-bottom: 2px;
  border-radius: 2px;
}

.mainGroupFunc {
  display: flex;
}

.subGroupFunc {
  width: 100%;
  display: inline;
  justify-content: center;
  text-align: center;
}

.icongroupfunc {
  padding: 16px;
  background-color: rgba(169, 169, 169, 0.192);
  border-radius: 100%;
}

.divider {
  height: 1px;
  background-color: #e0e0e048;
  margin: 16rpx 0;
}

.text-xl{
  font-size: 16px;
}

.text-lg{
  font-size: 14px;
}

.text-md{
  font-size: 14px;
}

.font-w-400{
  font-weight: 400;  
}

.font-w-500{
  font-weight: 500;  
}

.font-w-600{
  font-weight: 600;  
}


.option-item {
  padding: 12px 0;
  font-size: 16px;
  border-bottom: 1px solid #eee;
  color: rgb(0, 0, 0,0.5);
  text-align: center;
  background-color: white;
  width: 100%;
  font-weight: 400;
}
