/* pages/groupprofile/groupprofile.wxss */
.page-container{
  min-height: 100vh;
  background-color: rgba(202, 202, 202, 0.281);
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 10px 10px 10px 10px;
}
.sectionaction{
  padding-top: 40px;
}

.images{
  background-color: white;
  /* border: solid 0px rgb(156, 156, 156); */
  border-radius: 100%;
  height: 200px;
  width: 200px;
}

.sectionText{
  margin-top: 20px;
  width: 100%;
}

.boxtext{
  background: white;
  height: 50px;
  width: 100%;
  border-radius: 12px;
  display: flex;
  align-items: center;
}

.textbox{
padding: 0px 10px 0px 10px;
width: 100%;
height: 100%;
}

.rendertext{
  padding: 0px 10px 0px 10px;
  height: 100%;
  width: 100%;
  display: flex;
}

.buttonsubmit {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  background: rgb(36, 34, 161);
  color: white;
  font: 18px;
  font-weight: 600;
  height: 50px;
  border-radius: 12px;
}